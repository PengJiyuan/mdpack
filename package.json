{
  "name": "mdpack",
  "version": "0.2.0",
  "description": "markdown bundler",
  "main": "./lib/mdpack.js",
  "bin": "./bin/mdpack.js",
  "scripts": {
    "readme": "mdpack -e readme/index.md -p . -n README",
    "precommit": "npm run readme && mdpack -e readme/index.md -p readme/dist -n README -f all && git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PengJiyuan/mdpack.git"
  },
  "keywords": [
    "markdown",
    "bundle"
  ],
  "author": "PengJiyuan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PengJiyuan/mdpack/issues"
  },
  "homepage": "https://github.com/PengJiyuan/mdpack#readme",
  "dependencies": {
    "commander": "^2.15.1",
    "fs-extra": "^6.0.1",
    "highlight.js": "^9.12.0",
    "marked": "^0.4.0"
  },
  "devDependencies": {
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "precommit"
  ]
}
